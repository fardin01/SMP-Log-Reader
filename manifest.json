{
    "name": "SMP Log Reader",
    "version": "1.0",
    "description": "Readying SMP Logs like never before!",
    "manifest_version": 2,
    "chrome_url_overrides": {
        "newtab": "popup.html"
    },
    "permissions" : [
        "tabs"
    ],
    "background": {
        "scripts": [
            "js/jquery.min.js",
            "js/log_reader.js"
        ],
        "persistent": false
    },
    "browser_action": {

        "default_icon": {
            "16": "images/img16.png",
            "48": "images/img48.png"
        }
    },
    "icons": {
        "16": "images/img16.png",
        "48": "images/img48.png"
    }
}